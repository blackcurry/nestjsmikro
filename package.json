{
  "name": "community-api",
  "private": true,
  "version": "1.0.0",
  "description": "wehago community api",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:develop": "NODE_ENV=development node dist/main",
    "start:production": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=development jest --config ./test/jest-e2e.json",
    "deploy:dev": "pm2 start ecosystem.config.js --env development",
    "deploy:prod": "pm2 start ecosystem.config.js --env production",
    "deploy:close": "pm2 delete all",
    "mig:dev:create": "dotenv -e ./config/.development.env -- yarn mikro-orm migration:create"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.4.3",
    "@mikro-orm/nestjs": "^4.2.0",
    "@mikro-orm/postgresql": "^4.4.3",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/graphql": "^7.9.4",
    "@nestjs/mapped-types": "^0.1.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.9",
    "@types/crypto-js": "^4.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^4.4.3",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.17.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "rootDir": "./src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
