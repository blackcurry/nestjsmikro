{
  "moduleFileExtensions": ["js", "json", "ts"],
  "modulePaths": ["<rootDir>/src"],
  "rootDir": "../",
  "testRegex": "e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node",
  "transformIgnorePatterns": ["^.+\\.js$"]
}
